{
  "name": "typescript-serverless-template",
  "version": "1.1.0",
  "description": "TypeScript + Serverless Framework + 3 Musketeers",
  "scripts": {
    "prepare": "husky install",
    "test": "jest --verbose src/**/*.test.ts",
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npx eslint . --fix --ext .ts",
    "lint:watch": "npx esw . --watch --ext .ts",
    "dependencies:check": "npm-check -i ~* --skip-unused",
    "dependencies:update": "npm-check -u"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@types/aws-lambda": "^8.10.125",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "aws-sdk": "^2.1483.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-watch": "^8.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "npm-check": "^6.0.1",
    "prettier": "^3.0.3",
    "serverless-plugin-typescript": "^2.1.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
